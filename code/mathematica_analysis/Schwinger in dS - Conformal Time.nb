Cell[CellGroupData[{

Cell[TextData[{
 "For particles beyond each other' s horizon, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["ds", "2"], "\[GreaterEqual]", 
     FractionBox["1", 
      SuperscriptBox["H", 
       RowBox[{" ", "2"}]]]}], "\[Rule]", 
    RowBox[{"ds", "\[GreaterEqual]", 
     FractionBox["1", "H"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontWeight->"Bold",ExpressionUUID->"0100f43c-377e-4a3a-8501-0e9b747ad4c2"],
 " for constant conformal time ",
 StyleBox["(what do we mean by constant time?):",
  FontWeight->"Bold",
  FontSlant->"Italic"]
}], "Subsection",
 CellChangeTimes->{{3.7426076302448883`*^9, 3.742607637140428*^9}, {
   3.742607671250976*^9, 3.7426077214763107`*^9}, 3.742607783107896*^9, {
   3.7426087065893373`*^9, 
   3.7426087188668957`*^9}},ExpressionUUID->"1055302e-86f0-4fac-8fc6-\
6295ca0ad528"],

Cell[TextData[{
 "Given that \[Eta]  is unchanging, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"d\[Eta]", "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "bb2033d5-d2ae-491a-b3e1-99986271b103"],
 ", hence\t\t\t\t  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["ds", "2"], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       SuperscriptBox["dx", "2"], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", 
           RowBox[{"\[Eta]", "(", "T", ")"}]}], " ", "H"}], ")"}], "2"]], 
      "     ", "\[Rule]", "      ", "ds"}], "=", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["dx", 
        RowBox[{
         RowBox[{"\[Eta]", "(", "T", ")"}], " ", "H"}]]}], "\[GreaterEqual]", 
      
      FractionBox["1", "H"]}]}]}], TraditionalForm]],
  FontSize->24,ExpressionUUID->"4e597dd3-90b8-48c3-8b30-2c64a9f74e15"],
 ". \nNow\t\t\t\t\t\t\t\t\t\t\t           ",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    RowBox[{"ds", "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        FractionBox["dT", "dT"], 
        FractionBox["dx", 
         RowBox[{
          RowBox[{"\[Eta]", "(", "T", ")"}], " ", "H"}]]}], "\[GreaterEqual]", 
       RowBox[{
        FractionBox["1", "H"], "      ", "\[LongRightArrow]", "     ", 
        "ds"}]}], "=", 
      RowBox[{
       RowBox[{"dT", 
        FractionBox[
         RowBox[{
          RowBox[{"x", "'"}], 
          RowBox[{"(", "T", ")"}]}], 
         RowBox[{
          RowBox[{"\[Eta]", "(", "T", ")"}], " ", "H"}]]}], "\[GreaterEqual]", 
       FractionBox["1", "H"]}]}]}]}], TraditionalForm]],
  FontSize->24,ExpressionUUID->"de038b6c-c74d-42ae-a377-4b38815bcea4"],
 ".",
 StyleBox["\n",
  FontSize->24],
 StyleBox["Finally, integrating gives us:",
  FontSize->14],
 StyleBox[" ",
  FontSize->24],
 Cell[BoxData[
  FormBox[
   RowBox[{"s", "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Integral]", "0", "1"], 
      RowBox[{
       FractionBox[
        RowBox[{
         RowBox[{"x", "'"}], 
         RowBox[{"(", "T", ")"}]}], 
        RowBox[{
         RowBox[{"\[Eta]", "(", "T", ")"}], " ", "H"}]], 
       RowBox[{"\[DifferentialD]", "T"}]}]}], "\[GreaterEqual]", 
     FractionBox["1", "H"]}]}], TraditionalForm]],
  FontSize->24,ExpressionUUID->"c6e12b08-2812-4e26-95ec-ba78aad71e43"],
 ". Note that ",
 StyleBox["\[Eta](T)",
  FontSlant->"Italic"],
 " needs to be set to ",
 StyleBox["EB ",
  FontSlant->"Italic"],
 "since \[Eta] is not variable and because we want to find horizon in terms \
of particle separation at the end of the trajectory. Let us now evaluate this."
}], "Text",
